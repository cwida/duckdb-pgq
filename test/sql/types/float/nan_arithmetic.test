# name: test/sql/types/float/nan_arithmetic.test
# description: Test arithmetic on NaN values
# group: [float]

statement ok
PRAGMA enable_verification

foreach type FLOAT DOUBLE

foreach val nan

# any arithmetic on a nan value will result in a nan value

query I
SELECT '${val}'::${type} + 1
----
nan

query I
SELECT '${val}'::${type} - 1
----
nan

query I
SELECT '${val}'::${type} * 1
----
nan

query I
SELECT '${val}'::${type} / 1
----
nan

query I
SELECT '${val}'::${type} % 1
----
nan

query I
SELECT -('${val}'::${type})
----
nan

endloop

endloop


foreach type FLOAT DOUBLE

foreach val inf

# any arithmetic on a nan value will result in a nan value

query I
SELECT '${val}'::${type} + 1
----
inf


# inf + inf = inf
query I
SELECT '${val}'::${type} + '${val}'::${type} 
----
inf


query I
SELECT '${val}'::${type} - 1
----
inf

# inf - inf = nan
query I
SELECT '${val}'::${type} - '${val}'::${type} 
----
nan


query I
SELECT '${val}'::${type} * 1
----
inf

# inf * inf = inf
query I
SELECT '${val}'::${type} * '${val}'::${type}
----
inf

# inf * -inf = nan
query I
SELECT '${val}'::${type} * '-${val}'::${type}
----
-inf

query I
SELECT '${val}'::${type} / 1
----
inf

query I
SELECT 1 / '${val}'::${type}
----
0

query I
SELECT '${val}'::${type} % 1
----
nan

query I
SELECT 1 % '${val}'::${type}
----
1

query I
SELECT -('${val}'::${type})
----
-inf

endloop

endloop


foreach type FLOAT DOUBLE

foreach val -inf

# any arithmetic on a nan value will result in a nan value

query I
SELECT '${val}'::${type} + 1
----
-inf

query I
SELECT '${val}'::${type} - 1
----
-inf

query I
SELECT '${val}'::${type} * 1
----
-inf

query I
SELECT '${val}'::${type} / 1
----
-inf

query I
SELECT '${val}'::${type} % 1
----
nan

query I
SELECT -('${val}'::${type})
----
inf

endloop

endloop




