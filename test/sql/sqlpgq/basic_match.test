statement ok
CREATE PROPERTY GRAPH pg
VERTEX TABLES (
    Student PROPERTIES ( id, name ) LABEL Person
    )
EDGE TABLES (
    know    SOURCE KEY ( src ) REFERENCES Student ( id )
            DESTINATION KEY ( dst ) REFERENCES Student ( id )
            PROPERTIES ( createDate ) LABEL Knows
    );


query I
SELECT study.id
FROM GRAPH_TABLE (pg,
    MATCH
    (a:Person)
    COLUMNS (a.id)
    ) study
----
0
1
2
3
4

query II
SELECT study.a_id, study.name
FROM GRAPH_TABLE (pg,
    MATCH
    (a:Person)
    WHERE a.id = 0
    COLUMNS (a.id as a_id, a.name)
    ) study
----
0	Daniel


query II
SELECT study.a_name, study.b_name
FROM GRAPH_TABLE (pg,
    MATCH
    (a:Person)-[k:Knows]->(b:Person)
    WHERE a.id = 0
    COLUMNS (a.name as a_name, b.name as b_name)
    ) study;
----
Daniel	Tavneet
Daniel	Gabor
Daniel	Peter


